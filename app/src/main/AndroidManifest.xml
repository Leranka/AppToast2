<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.apptoast">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_pakgadi"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_pakgadi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" />
        <activity
            android:name=".Announcement"
            android:label="@string/title_activity_announcement" />
        <activity android:name=".Forgot_Password" />
        <activity android:name=".Destination" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".Profile" />
        <activity
            android:name=".LandingBottomActivity"
            android:label="@string/title_activity_landing_bottom" />
        <activity android:name=".SpecialTripsActivity"></activity>
    </application>

</manifest>